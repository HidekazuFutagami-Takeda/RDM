        AND ( cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term1 AND cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01'))
        OR    cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-09-01'))- bold.bold_term1 AND cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-09-01')) ) then '1'
    WHEN  bold.bold_joken2 = '3'                                                                                                                                            --BOLD譚｡莉ｶ2縲�ｦ門�
        AND (  cal.cal_date &gt;= TO_DATE(cal.cal_year || '-04-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '04-01') + bold.bold_term2
        OR     cal.cal_date &gt;= TO_DATE(cal.cal_year || '-10-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '10-01') + bold.bold_term2) then '1'
    WHEN bold.bold_joken2 = '4'                                                                                                                                             --BOLD譚｡莉ｶ2縲�ｦ匁忰
        AND (  cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term2 AND cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01'))
        OR     cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-09-01'))- bold.bold_term2 AND cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-09-01')) ) then '1'
    WHEN bold.bold_joken3 = '3'                                                                                                                                             --BOLD譚｡莉ｶ3縲�ｦ門�
        AND ( cal.cal_date &gt;= TO_DATE(cal.cal_year || '-04-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '-04-01') + bold.bold_term3
        OR    cal.cal_date &gt;= TO_DATE(cal.cal_year || '-10-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '-10-01') + bold.bold_term3) then '1'
    WHEN bold.bold_joken3 = '4'                                                                                                                                             --BOLD譚｡莉ｶ3縲�ｦ匁忰
        AND ( cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term3 AND cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01'))
        OR    cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-09-01'))- bold.bold_term3 AND cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-09-01')) ) then '1'
    WHEN bold.bold_joken1 = '5'                                                                                                                                             --BOLD譚｡莉ｶ1縲�ｹｴ蠎ｦ蛻�
        AND  cal.cal_date &gt;= TO_DATE(cal.cal_year || '-04-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '-04-01') + bold.bold_term1 then '1'
    WHEN bold.bold_joken1 = '6'                                                                                                                                             --BOLD譚｡莉ｶ1縲�ｹｴ蠎ｦ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term1 then '1'
    WHEN bold.bold_joken1 = '8'                                                                                                                                             --BOLD譚｡莉ｶ1縲�ｹｴ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) - bold.bold_term1 then '1'
    WHEN bold.bold_joken1 = '5'                                                                                                                                             --BOLD譚｡莉ｶ1縲�ｹｴ蠎ｦ蛻�
        AND  cal.cal_date &gt;= TO_DATE(cal.cal_year || '-04-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '-04-01') + bold.bold_term1 then '1'
    WHEN bold.bold_joken1 = '6'                                                                                                                                             --BOLD譚｡莉ｶ2縲�ｹｴ蠎ｦ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term1 then '1'
    WHEN bold.bold_joken1 = '8'                                                                                                                                             --BOLD譚｡莉ｶ2縲�ｹｴ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) - bold.bold_term1 then '1'
    WHEN bold.bold_joken2 = '5'                                                                                                                                             --BOLD譚｡莉ｶ2縲�ｹｴ蠎ｦ蛻�
        AND  cal.cal_date &gt;= TO_DATE(cal.cal_year || '-04-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '-04-01') + bold.bold_term2 then '1'
    WHEN bold.bold_joken2 = '6'                                                                                                                                             --BOLD譚｡莉ｶ2縲�ｹｴ蠎ｦ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term2 then '1'
    WHEN bold.bold_joken2 = '8'                                                                                                                                             --BOLD譚｡莉ｶ3縲�ｹｴ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) - bold.bold_term2 then '1'
    WHEN bold.bold_joken3 = '5'                                                                                                                                             --BOLD譚｡莉ｶ3縲�ｹｴ蠎ｦ蛻�
        AND  cal.cal_date &gt;= TO_DATE(cal.cal_year || '-04-01') AND cal.cal_date &lt; TO_DATE(cal.cal_year || '-04-01') + bold.bold_term3 then '1'
    WHEN bold.bold_joken3 = '6'                                                                                                                                             --BOLD譚｡莉ｶ3縲�ｹｴ蠎ｦ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-03-01')) - bold.bold_term3 then '1'
    WHEN bold.bold_joken3 = '8'                                                                                                                                             --BOLD譚｡莉ｶ3縲�ｹｴ譛ｫ
        AND  cal.cal_date &lt;= LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) AND cal.cal_date &gt; LAST_DAY(TO_DATE(cal.cal_year || '-12-01')) - bold.bold_term3 then '1'

