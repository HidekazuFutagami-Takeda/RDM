   <select id="selectCodeMst" parameterType="jp.co.takeda.rdm.entity.join.SelectCodeMstEntity" resultMap="selectCodeMstResultMap">
        SELECT
            DATA_KBN,                --データ区分
            DATA_CD,                 --コード
            DATA_NAME,               --名称
            DATA_VALUE,              --値
            DATA_SEQ,                --表示順
            DEF_FLG,                 --デフォルトフラグ
            BIKO                     --備考
        FROM
            IEI_MASTER.JKR_CD_MST               --従業員汎用マスタ
        WHERE
            DATA_KBN = #{inDataKbn} AND
            DATA_CD = #{inDataCd}
        ORDER BY DATA_SEQ
    </select>
